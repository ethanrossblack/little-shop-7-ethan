<h2>New Item Form</h2>

<%= form_with model: @item, url: merchant_items_path(@merchant, @item) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :unit_price %>
  <%= f.text_field :unit_price %>

  <%= f.submit %>
<% end %>